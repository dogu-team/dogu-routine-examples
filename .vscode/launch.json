  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: appium",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false,
      "args": [
        "--capture=no",
        "--rootdir=${workspaceFolder}/appium/python/pytest",
        "${workspaceFolder}/appium/python/pytest/app/test_android.py"
      ]
    },
    {
      "name": "Python: gamium",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": false,
      "args": [
        "--capture=no",
        "--rootdir=${workspaceFolder}/gamium/python/pytest",
        "${workspaceFolder}/gamium/python/pytest/app/test_dogurpgsample.py"
      ]
    },
    {
      "name": "Python: selenium",
      "type": "python",
      "request": "launch",
      "module": "pytest",
      "justMyCode": true,
      "cwd": "${workspaceFolder}/selenium/python/pytest",
      "args": [
        "--capture=no",
        "web/test_web.py"
      ]
    },
  ]
}
